<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/zm_gray_5"
    tools:context=".health.Activity.SeperateChallenge">

    <RelativeLayout
        android:layout_width="match_parent"
        android:background="@color/black"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/backpress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:src="@drawable/zm_btn_back_white" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/youra"
            android:layout_marginTop="20dp"
            android:layout_toRightOf="@+id/backpress"
            android:textColor="@color/white"
            android:fontFamily="@font/roboto_slab_bold"
            android:text="Join or Create Challenge"
            android:textSize="12sp"/>

        <Button
            android:id="@+id/button6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            android:textSize="10sp"
            android:background="@drawable/rounded_bg_home"
            android:padding="5dp"
            android:text=" Join using invite code "
            android:textAllCaps="false"
            android:textColor="@color/white" />
    </RelativeLayout>


    <ScrollView
        android:layout_width="match_parent"
        android:background="@color/zm_gray_5"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:background="@color/zm_gray_5"
            android:layout_height="wrap_content">

            <LinearLayout
                android:id="@+id/linearActiveChallenge"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/zm_gray_5"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/h1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"
                    android:fontFamily="@font/roboto_slab_bold"
                    android:text="FEATURED"
                    android:textColor="#203241"
                    android:textSize="25dp" />
                <LinearLayout
                    android:id="@+id/myevent"
                    android:layout_width="match_parent"
                    android:background="@color/zm_gray_5"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <HorizontalScrollView
                        android:id="@+id/horizontalScrollView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:focusableInTouchMode="true"
                        android:minHeight="150dp"
                        android:background="@color/zm_gray_5"

                        android:padding="5dp"
                        android:scrollbarSize="2dp"
                        android:scrollbars="none">

                        <LinearLayout
                            android:id="@+id/llayout1"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="10dp"
                            android:background="@color/zm_gray_5"
                            android:layout_marginRight="10dp"
                            android:divider="@drawable/vertical_divider"
                            android:dividerPadding="5dp"
                            android:orientation="horizontal"
                            android:paddingRight="10dp"
                            android:showDividers="middle"
                            android:weightSum="2" />


                    </HorizontalScrollView>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:visibility="gone"
                        android:layout_height="wrap_content"
                        android:padding="2dp">


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="VIEW ALL"
                            android:textColor="@color/home_blue"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintRight_toLeftOf="@+id/myevents_title_right_angle"
                            app:layout_constraintTop_toTopOf="parent" />

                        <cyd.awesome.material.AwesomeText
                            android:id="@+id/myevents_title_right_angle"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="5dp"
                            android:textColor="@color/home_blue"
                            android:textSize="20sp"
                            app:awesome_font="font_awesome"
                            app:awesome_icon="FA_ANGLE_RIGHT"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintRight_toLeftOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>


                </LinearLayout>
                <TextView
                    android:id="@+id/h2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"
                    android:fontFamily="@font/roboto_slab_bold"
                    android:text="JUST STARTED"
                    android:textColor="#203241"
                    android:textSize="25dp" />
                <LinearLayout
                    android:id="@+id/myevent1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <HorizontalScrollView
                        android:id="@+id/horizontalScrollView1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:focusableInTouchMode="true"
                        android:minHeight="150dp"
                        android:padding="5dp"
                        android:scrollbarSize="2dp"
                        android:scrollbars="none">

                        <LinearLayout
                            android:id="@+id/llayout11"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:divider="@drawable/vertical_divider"
                            android:dividerPadding="5dp"
                            android:orientation="horizontal"
                            android:paddingRight="10dp"
                            android:showDividers="middle"
                            android:weightSum="2" />


                    </HorizontalScrollView>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:visibility="gone"
                        android:padding="2dp">


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="VIEW ALL"
                            android:textColor="@color/home_blue"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintRight_toLeftOf="@+id/myevents_title_right_angle1"
                            app:layout_constraintTop_toTopOf="parent" />

                        <cyd.awesome.material.AwesomeText
                            android:id="@+id/myevents_title_right_angle1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="5dp"
                            android:textColor="@color/home_blue"
                            android:textSize="20sp"
                            app:awesome_font="font_awesome"
                            app:awesome_icon="FA_ANGLE_RIGHT"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintRight_toLeftOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>


                </LinearLayout>




            </LinearLayout>


        </RelativeLayout>

    </ScrollView>



</LinearLayout>

